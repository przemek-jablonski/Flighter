<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >

  <ImageView
      android:id="@+id/view_flights_browser_row_flight_icon"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="8dp"
      android:layout_marginTop="8dp"
      app:layout_constraintBottom_toTopOf="@+id/view_flights_browser_row_flight_from_text"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/ic_action_flight"
      />
  <TextView
      android:id="@+id/view_flights_browser_row_flight_arrival_text"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_marginEnd="8dp"
      android:layout_marginStart="8dp"
      android:ellipsize="end"
      android:lines="1"
      android:textSize="20sp"
      app:layout_constraintBottom_toBottomOf="@+id/view_flights_browser_row_flight_icon"
      app:layout_constraintEnd_toStartOf="@+id/view_flights_browser_row_flight_price_text"
      app:layout_constraintStart_toEndOf="@+id/view_flights_browser_row_flight_icon"
      app:layout_constraintTop_toTopOf="@+id/view_flights_browser_row_flight_icon"
      tools:text="Frankfurt International"
      />
  <TextView
      android:id="@+id/view_flights_browser_row_flight_price_text"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="8dp"
      android:layout_marginEnd="8dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="8dp"
      android:textSize="20sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.45"
      tools:text="~37"
      />
  <TextView
      android:id="@+id/view_flights_browser_row_flight_currency_text"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="8dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="@+id/view_flights_browser_row_flight_price_text"
      app:layout_constraintTop_toBottomOf="@+id/view_flights_browser_row_flight_price_text"
      app:layout_constraintVertical_bias="0.0"
      tools:text="PLN"
      />
  <TextView
      android:id="@+id/view_flights_browser_row_flight_arrival_date"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_marginBottom="8dp"
      android:layout_marginEnd="8dp"
      android:layout_marginStart="8dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toStartOf="@+id/view_flights_browser_row_flight_price_text"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/view_flights_browser_row_flight_from_text"
      tools:text="2018-08-08T13:15:00"
      />
  <TextView
      android:id="@+id/view_flights_browser_row_flight_from_text"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="8dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="8dp"
      android:text="from:"
      app:layout_constraintBottom_toTopOf="@+id/view_flights_browser_row_flight_arrival_date"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/view_flights_browser_row_flight_icon"
      />
  <TextView
      android:id="@+id/view_flights_browser_row_flight_departure_text"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_marginEnd="8dp"
      app:layout_constraintBottom_toBottomOf="@+id/view_flights_browser_row_flight_from_text"
      app:layout_constraintEnd_toStartOf="@+id/view_flights_browser_row_flight_price_text"
      app:layout_constraintStart_toStartOf="@+id/view_flights_browser_row_flight_arrival_text"
      app:layout_constraintTop_toTopOf="@+id/view_flights_browser_row_flight_from_text"
      tools:text="GÃ¶teborg Landvetter"
      />
</android.support.constraint.ConstraintLayout>